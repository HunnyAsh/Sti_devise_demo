<h1>Home</h1>
<h3>List Users</h3>
<table>
  <thead>
    <th>Email</th>
    <th>Type</th>
  </thead>
  <tbody>
    <% @users.each do |user| %>
      <tr>
        <td><%= user.email %></td>
        <td><%= user.type %></td>
      </tr>
    <% end %>
  </tbody>
</table>


<%= link_to 'Sign Out', destroy_user_session_path , method: :delete%>
